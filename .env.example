# Configuració del dispositiu Bluetti
# Substitueix XX:XX:XX:XX:XX:XX per l'adreça MAC real del teu dispositiu
BLUETTI_MAC=XX:XX:XX:XX:XX:XX
ENCRYPTION_KEY_FILE=encryption_keys.json

# Configuració MQTT
# Substitueix per les dades del teu broker MQTT
MQTT_HOST=192.168.1.100
MQTT_PORT=1883
MQTT_USERNAME=mqttuser
MQTT_PASSWORD=mqttpass
MQTT_TOPIC=bluetti

# Configuració de logging
LOG_LEVEL=info